<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.VBox?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ToggleGroup?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <fx:define>
        <Insets fx:id="margin" bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        <ToggleGroup fx:id="genderGroup"/>
    </fx:define>
    <children>
        <Label text="Gender" VBox.margin="$margin"/>
        <RadioButton mnemonicParsing="false" text="Male" toggleGroup="$genderGroup"/>
        <RadioButton mnemonicParsing="false" text="Female" toggleGroup="$genderGroup"/>
        <RadioButton mnemonicParsing="false" text="Unknown" toggleGroup="$genderGroup"/>
        <Button text="Close" VBox.margin="$margin"/>
    </children>
</VBox>
